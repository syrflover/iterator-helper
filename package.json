{
    "name": "@syrflover/iterator",
    "version": "0.0.52",
    "description": "Iterator Helper for TypeScript",
    "main": "dist/index.js",
    "module": "es/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "es",
        "dist"
    ],
    "scripts": {
        "start": "ITER_HELPER_LOG_LEVEL=trace ts-node -P tsconfig.json src/playground.ts",
        "test": "npm run build && npm run build:test && npm run test:cjs",
        "test:ts": "ts-mocha -p tsconfig.test.json test/**/*.test.ts test/**/**/*.test.ts test/**/**/**/*.test.ts",
        "test:cjs": "mocha test/__cjs/**/*.test.js test/__cjs/**/**/*.test.js test/__cjs/**/**/**/*.test.js",
        "build": "rm -rf ./dist && rm -rf ./es && node rollup.js",
        "build:test": "rm -rf ./test/__cjs && node test/rollup.js",
        "lint": "eslint src/**/*.ts",
        "doc": "typedoc --options typedoc.json",
        "prepublishOnly": "npm run lint && npm run test && npm run build"
    },
    "author": {
        "name": "Lee Tae Woo",
        "email": "syr@meu.works"
    },
    "bugs": {
        "url": "https://github.com/syrflover/iterator-helper/issues"
    },
    "repository": {
        "url": "https://github.com/syrflover/iterator-helper.git",
        "type": "git"
    },
    "license": "MIT",
    "keywords": [
        "async iterator",
        "iterator",
        "async"
    ],
    "devDependencies": {
        "@types/chai": "^4.2.3",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.2",
        "@typescript-eslint/eslint-plugin": "^2.5.0",
        "@typescript-eslint/parser": "^2.5.0",
        "chai": "^4.2.0",
        "eslint": "^6.5.1",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.18.2",
        "log4js": "^5.2.2",
        "mocha": "^6.2.2",
        "rollup": "^1.26.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-ignore": "^1.0.5",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-re": "^1.0.7",
        "rollup-plugin-typescript2": "^0.24.3",
        "ts-mocha": "^6.0.0",
        "ts-node": "^8.4.1",
        "typedoc": "^0.15.0",
        "typescript": "^3.7.1-rc"
    }
}
