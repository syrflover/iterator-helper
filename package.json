{
    "name": "@syrflover/iterator",
    "version": "0.0.1",
    "description": "Iterator Helper for TypeScript",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "start": "ITER_HELPER_LOG_LEVEL=trace ts-node -P tsconfig.json src/playground.ts",
        "test": "tsc -p tsconfig.test.json && eslint test/**/*.ts && ts-mocha -p tsconfig.test.json test/**/*.test.ts test/**/**/*.test.ts",
        "build": "rm -rf ./build && tsc -p tsconfig.build.json",
        "lint": "eslint src/**/*.ts",
        "prepublishOnly": "npm run lint && npm run test && npm run build"
    },
    "author": {
        "name": "Lee Tae Woo"
    },
    "repository": {
        "url": "https://github.com/syrflover/iterator-helper.git",
        "type": "git"
    },
    "license": "MIT",
    "keywords": [
        "iterator",
        "iterator helper"
    ],
    "dependencies": {
        "log4js": "^5.2.2"
    },
    "devDependencies": {
        "@types/chai": "^4.2.3",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.2",
        "@typescript-eslint/eslint-plugin": "^2.5.0",
        "@typescript-eslint/parser": "^2.5.0",
        "chai": "^4.2.0",
        "eslint": "^6.5.1",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.18.2",
        "mocha": "^6.2.2",
        "ts-mocha": "^6.0.0",
        "ts-node": "^8.4.1",
        "tslint": "^5.20.0",
        "tslint-config-airbnb": "^5.11.2",
        "typescript": "^3.7.0-beta"
    }
}
