{
    "name": "@syrflover/iterator",
    "version": "0.2.3",
    "description": "Iterator Helper for TypeScript",
    "main": "dist/mod.js",
    "types": "dist/mod.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "deno run --config tsconfig.json src/playground.ts --ITER_HELPER_LOG",
        "test": "npm run build:deno && deno run --allow-run --allow-read scripts/test.ts",
        "test:reload": "npm run build:deno && npm run test reload",
        "build:node": "rm -rf ./dist && node scripts/node_build.js",
        "build:deno": "rm -rf ./deno && deno run --allow-read --allow-write scripts/deno_build.ts",
        "lint": "eslint src/**/*.ts",
        "fmt": "deno run --allow-run --allow-read scripts/fmt.ts",
        "doc": "deno --allow-run --allow-write scripts/docs.ts",
        "doc:master": "deno --allow-run scripts/docs.ts onlyMaster",
        "prepublishOnly": "npm run lint && npm run fmt && npm run test:reload && npm run build:node && npm run build:deno && npm run fmt publish",
        "postpublish": "npm run doc"
    },
    "author": {
        "name": "Lee Tae Woo",
        "email": "syr@meu.works"
    },
    "bugs": {
        "url": "https://github.com/syrflover/iterator-helper/issues"
    },
    "repository": {
        "url": "https://github.com/syrflover/iterator-helper.git",
        "type": "git"
    },
    "license": "MIT",
    "keywords": [
        "async iterator",
        "iterator",
        "async"
    ],
    "devDependencies": {
        "@types/node": "^12.12.7",
        "@typescript-eslint/eslint-plugin": "^2.7.0",
        "@typescript-eslint/parser": "^2.7.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.18.2",
        "rollup": "^1.26.5",
        "rollup-plugin-ignore": "^1.0.5",
        "rollup-plugin-re": "^1.0.7",
        "rollup-plugin-typescript2": "^0.25.2",
        "typedoc": "^0.15.0",
        "typescript": "^3.7.2",
        "typescript-deno-plugin": "^1.2.7"
    }
}
